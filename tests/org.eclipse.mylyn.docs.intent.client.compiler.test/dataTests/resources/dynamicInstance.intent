@M
Resource R1 {
	URI = "R1";
	content += p1;
}
new EPackage p1 {
	nsURI = "p1URI";
	nsPrefix = "p1Prefix";
	eClassifiers += new EClass Dynamic {
		eStructuralFeatures += new EAttribute {
			name = "name";
			eType = EString;
			changeable = "true";
			ordered = "true";
		};
	};
}
Resource R2 {
	URI = "R2";
	content += instance;
}
new Dynamic instance {
	name = "instance";
}
M@