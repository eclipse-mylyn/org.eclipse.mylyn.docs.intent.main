@M
Resource R1 {
	URI = "R1";
	content += p1;
}
new EPackage p1 {
	nsURI = "p1URI";
	nsPrefix = "p1Prefix";
	eClassifiers += new EDataType TestType {
		instanceTypeName = "java.lang.String";
	};
	eClassifiers += new EClass Dynamic {
		eStructuralFeatures += new EAttribute {
			name = "test";
			eType = TestType;
			changeable = "true";
			ordered = "true";
		};
	};
}
M@