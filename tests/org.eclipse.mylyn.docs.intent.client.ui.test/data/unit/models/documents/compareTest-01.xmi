      <intentContent xsi:type="intentDU:DescriptionUnit">
        <instructions xsi:type="intentDU:DescriptionBloc" lineBreak="true">
          <descriptionBloc xsi:type="markup:SimpleContainer">
            <content xsi:type="markup:Paragraph">
              <attributes/>
              <content xsi:type="markup:Text" data="During the Match process, deleted or added elements are considered as unmatched."/>
            </content>
          </descriptionBloc>
        </instructions>
        <instructions xsi:type="intentDU:DescriptionBloc" lineBreak="true">
          <descriptionBloc xsi:type="markup:SimpleContainer">
            <content xsi:type="markup:Paragraph">
              <attributes/>
              <content xsi:type="markup:Text" data="Two different situations can occur : "/>
            </content>
          </descriptionBloc>
        </instructions>
        <instructions xsi:type="intentDU:DescriptionBloc" lineBreak="true">
          <descriptionBloc xsi:type="markup:SimpleContainer">
            <content xsi:type="markup:List">
              <attributes/>
              <items>
                <attributes/>
                <content xsi:type="markup:Text" data="an element defined in the left model is not matching any element in the right model (deleted element)"/>
              </items>
              <items>
                <attributes/>
                <content xsi:type="markup:Text" data="an element defined in the right model is not matching any element in the left model (added element)"/>
              </items>
            </content>
          </descriptionBloc>
        </instructions>
        <instructions xsi:type="intentDU:DescriptionBloc">
          <descriptionBloc xsi:type="markup:SimpleContainer">
